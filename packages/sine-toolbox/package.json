{
	"author": {
		"email": "hi@adityaborkar.com",
		"name": "Aditya Borkar",
		"url": "https://adityaborkar.com"
	},
	"bugs": {
		"url": "https://github.com/AdityaBorkar/sine-toolbox/issues"
	},
	"dependencies": {
		"arktype": "^2.1.20",
		"commander": "^14.0.0"
	},
	"description": "A collection of tools for Sine",
	"devDependencies": {
		"@types/bun": "latest"
	},
	"displayName": "Sine Toolbox",
	"files": [
		"LICENSE.md",
		"./dist",
		"README.md",
		"package.json"
	],
	"homepage": "https://github.com/AdityaBorkar/sine-toolbox#readme",
	"keywords": [
		"sine mods",
		"zen mods",
		"toolbox",
		"tools"
	],
	"license": "MIT",
	"module": "./dist/src/index.js",
	"name": "sine-toolbox",
	"private": false,
	"repository": {
		"directory": "packages/sine-toolbox",
		"type": "git",
		"url": "https://github.com/AdityaBorkar/sine-toolbox"
	},
	"scripts": {
		"build": "zshy",
		"check:code": "biome check --write --diagnostic-level=error",
		"check:types": "tsc --noEmit",
		"dev": "zshy --watch",
		"publish": "bun publish --provenance"
	},
	"type": "module",
	"version": "0.0.1",
	"zshy": {
		"bin": {
			"sine-toolbox": "./src/cli/index.ts"
		},
		"exports": {
			".": "./src/index.ts"
		}
	},
	"main": "./dist/src/index.cjs",
	"types": "./dist/src/index.d.cts",
	"exports": {
		".": {
			"types": "./dist/src/index.d.cts",
			"import": "./dist/src/index.js",
			"require": "./dist/src/index.cjs"
		}
	},
	"bin": {
		"sine-toolbox": "./dist/src/cli/index.cjs"
	}
}
